exec spv_print_header
host sha256sum &&p_script_validador

begin
	
	dbms_output.put_line('===> Validando tablas');
	spv_verifica_tablas;

	dbms_output.put_line('===> Validando delete completo al inicio');
	spv_verifica_delete;

	dbms_output.put_line('===> Validando insert');
	spv_verifica_insert;

	dbms_output.put_line('===> Validando update');
	spv_verifica_update;

	savepoint sv1;

	dbms_output.put_line('===> Validando delete por registro');
	spv_verifica_delete_registro;

	
	rollback to sv1;

	
	commit;

	dbms_output.put_line('===> validando Binarios');
	spv_valida_binarios;

	dbms_output.put_line('===> Validando delete completo al final');
	spv_verifica_delete;

  spv_print_ok('Validaci√≥n concluida');

	commit;
exception
	when others then
	dbms_output.put_line(dbms_utility.format_error_backtrace);
	rollback;
	raise;
end;
